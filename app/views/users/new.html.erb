<h1>SignUp</h1>
<p>Find me in app/views/users/new.html.erb</p>


<%= form_for @user, :html => { :multipart => true }  do |form| %>
  
  <%= form.file_field :avatar %>

    <div class="actions">
      <%= form.submit "Upload" %>
    </div>

    <div class="field">
    <%= form.label :name %><br />
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :email %><br />
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %><br />
    <%= form.password_field :password %>
  </div>
  <div class="field">
    <%= form.label :password_confirmation, "Confirmation" %><br />
    <%= form.password_field :password_confirmation %>
  </div>
  <div class="actions">
    <%= form.submit "Sign up" %>
  </div>





<% end %>